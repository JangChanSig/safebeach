<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="intujuku.beachalarm">
    <!-- pce -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_square"
        android:label="안전한 휴가"
        android:roundIcon="@mipmap/ic_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name="intujuku.beachalarm.IntroActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="intujuku.beachalarm.MainActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name="intujuku.beachalarm.MapListActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="intujuku.beachalarm.D2Activity"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"/>
        <activity
            android:name="intujuku.beachalarm.InfoActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"/> <!-- pce -->
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyDZPZjZ2VxDSI3N-JT970CuZVG3_ppm35c" />

        <uses-library android:name="org.apache.http.legacy" android:required="false"/>

    </application>

</manifest>